        "tmp-files-directory": "./tmp_generated_files",
        "chrome-webdriver": null,
        "firefox-webdriver": null,
        "safari-webdriver": null
    },

    "target-browser-variant": "firefox_dynamic_reflow_roots",

    "style-weights": {
